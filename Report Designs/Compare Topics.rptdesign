                        <detail>
                            <row id="464">
                                <method name="onCreate"><![CDATA[var myRow = [];

myRow.canid = this.getRowData().getColumnValue("candidateid");
myRow.topid = this.getRowData().getColumnValue("topicid");
myRow.topic = this.getRowData().getColumnValue("topic");
myRow.pos = this.getRowData().getColumnValue("positive");
myRow.neg = this.getRowData().getColumnValue("negative");
myRow.neu = this.getRowData().getColumnValue("neutral");
myRow.tot = parseInt(myRow["pos"]) + parseInt(myRow["neg"]) +  parseInt(myRow["neu"]);

myDataSet.push(myRow);]]></method>
                                <cell id="465"/>
                            </row>
                        </detail>
                        <footer>
                            <row id="564">
                                <method name="onCreate"><![CDATA[var myRow = [];

for (var i=0;i<myDataSet.length;i++) {
	if (myDataSet[i].tot > maxTotal) 
		maxTotal = myDataSet[i].tot;
}	
	
for (var i=0;i<myDataSet.length;i++) {
	
	if (i != (myDataSet.length - 1)) {

		if (myDataSet[i].topid == myDataSet[i+1].topid) {
			//Insert 1 row from 2 source rows
			myRow.topid = myDataSet[i].topid;
			myRow.topic = myDataSet[i].topic;
			myRow.c1 = myDataSet[i].canid;
			myRow.pos1 = myDataSet[i].pos;
			myRow.neg1 = myDataSet[i].neg;
			myRow.neu1 = myDataSet[i].neu;
			myRow.tot1 = myDataSet[i].tot;
			myRow.c2 = myDataSet[i+1].canid;
			myRow.pos2 = myDataSet[i+1].pos;
			myRow.neg2 = myDataSet[i+1].neg;
			myRow.neu2 = myDataSet[i+1].neu;
			myRow.tot2 = myDataSet[i+1].tot;
		
			myNewDataSet.push(myRow);
			i++;
		}
		
		else {//Different topics
			if (myDataSet[i].canid == params["pCandidateId1"].value) {
				myRow.topid = myDataSet[i].topid;
				myRow.topic = myDataSet[i].topic;
				myRow.c1 = params["pCandidateId1"].value;
				myRow.pos1 = myDataSet[i].pos;
				myRow.neg1 = myDataSet[i].neg;
				myRow.neu1 = myDataSet[i].neu;
				myRow.tot1 = myDataSet[i].tot;
				myRow.c2 = params["pCandidateId2"].value;
				myRow.pos2 = 0;
				myRow.neg2 = 0;
				myRow.neu2 = 0;
				myRow.tot2 = 0;
		
				myNewDataSet.push(myRow);
			}
			else {
				myRow.topid = myDataSet[i].topid;
				myRow.topic = myDataSet[i].topic;
				myRow.c1 = params["pCandidateId1"].value;
				myRow.pos1 = 0;
				myRow.neg1 = 0;
				myRow.neu1 = 0;
				myRow.tot1 = 0;
				myRow.c2 = params["pCandidateId2"].value;
				myRow.pos2 = myDataSet[i].pos;
				myRow.neg2 = myDataSet[i].neg;
				myRow.neu2 = myDataSet[i].neu;
				myRow.tot2 = myDataSet[i].tot;
		
				myNewDataSet.push(myRow);			
			
			}
		}

		myRow = [];
	}
}
myDataSet = [];]]></method>
